    "@radix-ui/react-toggle": "npm:@radix-ui/react-toggle@^1.1.10",
    "@radix-ui/react-toggle-group": "npm:@radix-ui/react-toggle-group@^1.1.11",
    "@radix-ui/react-tooltip": "npm:@radix-ui/react-tooltip@^1.2.8",
    "@radix-ui/react-use-callback-ref": "npm:@radix-ui/react-use-callback-ref@^1.1.1",
    "@radix-ui/react-use-controllable-state": "npm:@radix-ui/react-use-controllable-state@^1.2.2",
    "@radix-ui/react-use-effect-event": "npm:@radix-ui/react-use-effect-event@^0.0.2",
    "@radix-ui/react-use-escape-keydown": "npm:@radix-ui/react-use-escape-keydown@^1.1.1",
    "@radix-ui/react-use-is-hydrated": "npm:@radix-ui/react-use-is-hydrated@^0.1.0",
    "@radix-ui/react-use-layout-effect": "npm:@radix-ui/react-use-layout-effect@^1.1.1",
    "@radix-ui/react-use-previous": "npm:@radix-ui/react-use-previous@^1.1.1",
    "@radix-ui/react-use-size": "npm:@radix-ui/react-use-size@^1.1.1",
    "@radix-ui/react-visually-hidden": "npm:@radix-ui/react-visually-hidden@^1.2.3",
    "sonner": "npm:sonner@^2.0.7",
    "tailwind-merge": "npm:tailwind-merge@^3.3.1",
    "tw-animate-css": "npm:tw-animate-css@^1.3.7",
    "vite": "npm:vite@^7.1.4",
    "tailwindcss": "npm:tailwindcss@^4.1.12",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.1.12"
  },
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.asynciterable",
      "dom.iterable",
      "deno.ns"
    ],
    "jsx": "precompile",
    "jsxImportSource": "preact",
    "jsxPrecompileSkipElements": [
      "a",
      "img",
      "source",
      "body",
      "html",
      "head",
      "title",
      "meta",
      "script",
      "link",
      "style",
      "base",
      "noscript",
      "template"
    ]
  }
}
