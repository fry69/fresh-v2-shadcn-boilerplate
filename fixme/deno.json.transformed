{
  "nodeModulesDir": "auto",
  "tasks": {
    "check": "deno fmt --check . && deno lint . && deno check",
    "dev": "vite",
    "build": "vite build",
    "start": "deno serve -A _fresh/server.js",
    "update": "deno run -A -r jsr:@fresh/update .",
    "check-deps": "deno run -A jsr:@check/deps --allow-unused"
  },
  "lint": {
    "rules": {
      "tags": [
        "fresh",
        "recommended"
      ]
    }
  },
  "exclude": [
    "**/_fresh/*"
  ],
  "imports": {
    "@/": "./",
    "class-variance-authority": "npm:class-variance-authority@^0.7.1",
    "clsx": "npm:clsx@^2.1.1",
    "cmdk": "npm:cmdk@^1.1.1",
    "fresh": "jsr:@fresh/core@^2.0.0-beta.3",
    "@fresh/plugin-vite": "jsr:@fresh/plugin-vite@^0.9.11",
    "lucide-preact": "npm:lucide-preact@^0.542.0",
    "postcss": "npm:postcss@^8.5.6",
    "preact": "npm:preact@^10.27.1",
    "@preact/signals": "npm:@preact/signals@^2.3.1",
    "@radix-ui/react-accordion": "npm:@radix-ui/react-accordion@^1.2.12",
    "@radix-ui/react-alert-dialog": "npm:@radix-ui/react-alert-dialog@^1.1.15",
    "@radix-ui/react-arrow": "npm:@radix-ui/react-arrow@^1.1.7",
    "@radix-ui/react-aspect-ratio": "npm:@radix-ui/react-aspect-ratio@^1.1.7",
    "@radix-ui/react-avatar": "npm:@radix-ui/react-avatar@^1.1.10",
    "@radix-ui/react-checkbox": "npm:@radix-ui/react-checkbox@^1.3.3",
    "@radix-ui/react-collapsible": "npm:@radix-ui/react-collapsible@^1.1.12",
    "@radix-ui/react-collection": "npm:@radix-ui/react-collection@^1.1.7",
    "@radix-ui/react-compose-refs": "npm:@radix-ui/react-compose-refs@^1.1.2",
    "@radix-ui/react-context": "npm:@radix-ui/react-context@^1.1.2",
    "@radix-ui/react-context-menu": "npm:@radix-ui/react-context-menu@^2.2.16",
    "@radix-ui/react-dialog": "npm:@radix-ui/react-dialog@^1.1.15",
    "@radix-ui/react-direction": "npm:@radix-ui/react-direction@^1.1.1",
    "@radix-ui/react-dismissable-layer": "npm:@radix-ui/react-dismissable-layer@^1.1.11",
    "@radix-ui/react-dropdown-menu": "npm:@radix-ui/react-dropdown-menu@^2.1.16",
    "@radix-ui/react-focus-guards": "npm:@radix-ui/react-focus-guards@^1.1.3",
    "@radix-ui/react-focus-scope": "npm:@radix-ui/react-focus-scope@^1.1.7",
    "@radix-ui/react-hover-card": "npm:@radix-ui/react-hover-card@^1.1.15",
    "@radix-ui/react-id": "npm:@radix-ui/react-id@^1.1.1",
    "@radix-ui/react-label": "npm:@radix-ui/react-label@^2.1.7",
    "@radix-ui/react-menu": "npm:@radix-ui/react-menu@^2.1.16",
    "@radix-ui/react-menubar": "npm:@radix-ui/react-menubar@^1.1.16",
    "@radix-ui/react-navigation-menu": "npm:@radix-ui/react-navigation-menu@^1.2.14",
    "@radix-ui/react-popover": "npm:@radix-ui/react-popover@^1.1.15",
